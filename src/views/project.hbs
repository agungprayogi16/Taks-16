<html lang="en">

  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.1/dist/css/bootstrap.min.css"
      rel="stylesheet"
      integrity="sha384-4bw+/aepP/YC94hEpVNVgiZdgIC5+VKNBQNGCHeKRQN+PtmoHDEXuppvnDJzQIu9"
      crossorigin="anonymous"
    />
    <script
      src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.1/dist/js/bootstrap.bundle.min.js"
      integrity="sha384-HwwvtgBNo3bZJJLYd8oVXjrBZt8cqVSpeBNS5n7C8IVInixGAoxmnlMuBnhbgrkm"
      crossorigin="anonymous"
    ></script>
    <link
      rel="stylesheet"
      href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.2/css/all.min.css"
      integrity="sha512-z3gLpd7yknf1YoNbCzqRKc4qyor8gaKU1qmn+CShxbuBusANI9QpRohGBreCFkKxLhei6S9CQXFEbbKuqLg0DA=="
      crossorigin="anonymous"
      referrerpolicy="no-referrer"
    />
    <link rel="stylesheet" href="css/blog.css" />
    <link rel="icon" href="image/123.jpeg" type="image/x-icon" />
    <title>Taks Agung prayogi purba</title>
  </head>

  <body>
    <nav class="navbar navbar-expand-lg bg-dark">
      <div class="container-fluid">
        <a class="navbar-brand" href="#">
          <img
            src="image/logo-removebg-preview.png"
            alt="logo"
            width="60"
            height="50"
          />
        </a>
        <button
          class="navbar-toggler bg-light align-center"
          type="button"
          data-bs-toggle="collapse"
          data-bs-target="#navbarSupportedContent"
          aria-controls="navbarSupportedContent"
          aria-expanded="false"
          aria-label="Toggle navigation"
        >
          <span class="navbar-toggler-icon"></span>
        </button>
        <div
          class="collapse navbar-collapse text-center"
          id="navbarSupportedContent"
        >
          <ul class="navbar-nav me-auto mb-2 mb-lg-0">

            <li class="nav-item">
              <a class="nav-link text-light" href="/">Home</a>
            </li>
            <li class="nav-item">
              <a class="nav-link text-warning" href="/add-project">Add Project</a>
            </li>
            <li class="nav-item">
              <a class="nav-link text-light" href="/testimoni">Testimoni</a>

            </li>
            <ul class="dropdown-menu bg-light">
              <li><a class="dropdown-item text-warning" href="/">Home</a></li>
              <li><a class="dropdown-item text-light" href="/add-project">Add
                  Project</a></li>
              <li><a
                  class="dropdown-item text-light"
                  href="/testimoni"
                >Testimoni</a></li>
              <li><a
                  class="dropdown-item text-light"
                  href="/from-register"
                >register </a></li>
              <li><a
                  class="dropdown-item text-light"
                  href="/from-login"
                >Login</a></li>
              <li><a
                  class="dropdown-item text-light"
                  href="/from-logout"
                >Logout</a></li>
              <li><a class="dropdown-item text-light" href="/contact">Contact me</a></li>
            </ul>

          </ul>

          <div class="d-flex justify-content-center">
            <ul class="navbar-nav">
              {{#if isLogin}}
                <li class="nav-item">
                  <a
                    class="nav-link text-danger active fw-semibold"
                    href="#"
                  >Hello, {{user}}</a>
                </li>
                <li class="nav-item">
                  <a class="nav-link text-light" href="/from-logout">Logout</a>
                </li>
              {{else}}
                <li class="nav-item">
                  <a
                    class="nav-link text-light"
                    href="/from-register"
                  >Register</a>
                </li>
                <li class="nav-item ms-3">
                  <a class="nav-link text-light" href="/from-login">Login</a>
                </li>
              {{/if}}
              <a class="btn btn-danger btn-submit" href="/contact">Contact Me</a>
            </ul>
          </div>
        </div>
      </div>

    </nav>
    <div class="container mt-5 d-flex">
      <form
        method="post"
        action="/project"
        class="container mt-3 bg-light pr-3 pl-3 pt-3"
        id="list-project-container"
        enctype="multipart/form-data"
      >
        <h2 style="margin: 25px; text-align: center; font-size: 28px">
          ADD MY PROJECT
        </h2>
        <div class="form-group">
          <label> Project Name </label>
          <input id="input-project-name" name="name" class="form-control" />
        </div>
        <div class="d-flex justify-content-between mt-3">
          <div>
            <label> Start Date </label>
            <input
              id="input-start-date"
              name="start_date"
              type="date"
              class="form-control"
            />
          </div>

          <div>
            <label> End Date </label>
            <input
              id="input-end-date"
              name="end_date"
              type="date"
              class="form-control w-100"
            />
          </div>
        </div>
        <div class="mt-3">
          <label for=" description">Description:</label>
          <textarea
            class="form-control"
            id="input-description"
            placeholder="Tulis deskripsi Anda"
            name="description"
            rows="4"
          ></textarea>
        </div>
        <div class="container mt-3">
          <div class="upload-blog">
            <div class="form-detail">
              <label> Technologies </label>
              <div class="form-detail-technologies">
                <div class="input-checkbox">
                  <input
                    type="checkbox"
                    id="checkbox-javascript"
                    value="true"
                    class="input-technologies"
                    name="javascript"
                  />
                  <label for="nodejs">JavaScript</label>
                </div>
                <div class="input-checkbox">
                  <input
                    type="checkbox"
                    id="checkbox-nextjs"
                    value="true"
                    class="input-technologies"
                    name="vuejs"
                  />
                  <label for="reactjs">vue js</label>
                </div>
                <div class="input-checkbox">
                  <input
                    type="checkbox"
                    id="checkbox-nodejs"
                    value="true"
                    class="input-technologies"
                    name="nodejs"
                  />
                  <label for="checkbox-nodeja">Node js</label>
                </div>
                <div class="input-checkbox">
                  <input
                    type="checkbox"
                    id="jcheckbox-reactjs"
                    value="true"
                    class="input-technologies"
                    name="reactjs"
                  />
                  <label for="checkbox-reactjs">React Js</label>
                </div>
              </div>
            </div>
            <div class="form-detail">
              <label> Upload Image </label>
              <input
                placeholder="Attach File"
                type="file"
                id="filename"
                name="filename"
              />
            </div>

            <div class="row mb-3 d-flex justify-content-end">
              <div class="col-sm-4">
                <button
                  class="btn btn-primary rounded-pill px-5"
                  type="submit"
                >Submit</button>
              </div>
            </div>

          </div>
        </div>
      </form>

    </div>
    <script
      src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.1/dist/js/bootstrap.bundle.min.js"
      integrity="sha384-HwwvtgBNo3bZJJLYd8oVXjrBZt8cqVSpeBNS5n7C8IVInixGAoxmnlMuBnhbgrkm"
      crossorigin="anonymous"
    ></script>

  </body>

</html>