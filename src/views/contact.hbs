<html lang="en">

  <head>
    <!-- Tautan ke Bootstrap CSS -->
    <link
      rel="stylesheet"
      href="https://maxcdn.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css"
    />
    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.1/dist/css/bootstrap.min.css"
      rel="stylesheet"
      integrity="sha384-4bw+/aepP/YC94hEpVNVgiZdgIC5+VKNBQNGCHeKRQN+PtmoHDEXuppvnDJzQIu9"
      crossorigin="anonymous"
    />

    <!-- Tautan ke Bootstrap JavaScript (termasuk jQuery) -->
    <script
      src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"
    ></script>
    <script
      src="https://maxcdn.bootstrapcdn.com/bootstrap/4.5.2/js/bootstrap.min.js"
    ></script>
    <script
      src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.1/dist/js/bootstrap.bundle.min.js"
      integrity="sha384-HwwvtgBNo3bZJJLYd8oVXjrBZt8cqVSpeBNS5n7C8IVInixGAoxmnlMuBnhbgrkm"
      crossorigin="anonymous"
    ></script>
    <link rel="stylesheet" href="css/contct.css" />

    <link rel="icon" href="image/123.jpeg" type="image/x-icon" />
    <title>Taks Agung prayogi purba</title>
  </head>

  <body>
    <nav class="navbar navbar-expand-lg bg-dark">
      <div class="container-fluid">
        <a class="navbar-brand" href="#">
          <img
            src="image/logo-removebg-preview.png"
            alt="logo"
            width="60"
            height="50"
          />
        </a>
        <button
          class="navbar-toggler bg-light align-center"
          type="button"
          data-bs-toggle="collapse"
          data-bs-target="#navbarSupportedContent"
          aria-controls="navbarSupportedContent"
          aria-expanded="false"
          aria-label="Toggle navigation"
        >
          <span class="navbar-toggler-icon"></span>
        </button>
        <div
          class="collapse navbar-collapse text-center"
          id="navbarSupportedContent"
        >
          <ul class="navbar-nav me-auto mb-2 mb-lg-0">

            <li class="nav-item">
              <a class="nav-link text-light" href="/">Home</a>
            </li>
            <li class="nav-item">
              <a class="nav-link text-light" href="/add-project">Add Project</a>
            </li>
            <li class="nav-item">
              <a class="nav-link text-light" href="/testimoni">Testimoni</a>

            </li>
            <ul class="dropdown-menu bg-light">
              <li><a class="dropdown-item text-warning" href="/">Home</a></li>
              <li><a class="dropdown-item text-light" href="/add-project">Add
                  Project</a></li>
              <li><a
                  class="dropdown-item text-light"
                  href="/testimoni"
                >Testimoni</a></li>
              <li><a
                  class="dropdown-item text-light"
                  href="/from-register"
                >register </a></li>
              <li><a
                  class="dropdown-item text-light"
                  href="/from-login"
                >Login</a></li>
              <li><a
                  class="dropdown-item text-light"
                  href="/from-logout"
                >Logout</a></li>
              <li><a class="dropdown-item text-warning" href="/contact">Contact
                  me</a></li>
            </ul>

          </ul>

          <div class="d-flex justify-content-center">
            <ul class="navbar-nav">
              {{#if isLogin}}
                <li class="nav-item">
                  <a
                    class="nav-link text-danger active fw-semibold"
                    href="#"
                  >Hello, {{user}}</a>
                </li>
                <li class="nav-item">
                  <a class="nav-link text-light" href="/from-logout">Logout</a>
                </li>
              {{else}}
                <li class="nav-item">
                  <a
                    class="nav-link text-light"
                    href="/from-register"
                  >Register</a>
                </li>
                <li class="nav-item ms-3">
                  <a class="nav-link text-light" href="/from-login">Login</a>
                </li>
              {{/if}}
              <a class="btn btn-danger btn-submit" href="/contact">Contact Me</a>
            </ul>
          </div>
        </div>
      </div>

    </nav>
    {{#if messages.success}}
      <div class="alert alert-success" role="alert">
        <div class="container">
          {{messages.success}}
        </div>
      </div>
    {{/if}}

    <div class="container">
      <h2 style="text-align: center;" class="mt-4">Get in touct</h2>
      <form class="container mt-3 bg-light pr-3 pl-3 pt-3">
        <div class="form-group">
          <label for="nama">Nama:</label>
          <input
            type="text"
            class="form-control"
            id="nama"
            placeholder="Masukkan nama"
            name="nama"
          />
        </div>
        <div class="form-group">
          <label for="email">Email:</label>
          <input
            type="email"
            class="form-control"
            id="email"
            placeholder="Masukkan email"
            name="email"
          />
        </div>
        <div class="mb-3">
          <label for="exampleFormControlInput1" class="form-label">phone number</label>
          <input
            type="tel"
            class="form-control"
            id="exampleFormControlInput1"
            placeholder="0823xxx"
          />
        </div>
        <div class="mb-3">
          <label for="input-select">Subject</label>
          <select name="input-select" class="form-control" id="input-select">
            <option value="hidden">Select option</option>
            <option value="">Frontend deplover</option>
            <option value="">Backend deplover</option>
            <option value="">Fullstack deplover</option>
          </select>
        </div>
        <div class="form-group">
          <label for="pesan">Pesan:</label>
          <textarea
            class="form-control"
            id="pesan"
            placeholder="Tulis pesan Anda"
            name="pesan"
            rows="4"
          ></textarea>
        </div>
        <div class="d-flex justify-content-end">
          <button type="submit" class="btn btn-dark">Send</button>
        </div>
      </form>
    </div>
    <script src="../assets/js/script.js"></script>

  </body>

</html>